{
  "name": "roar-dashboard",
  "private": true,
  "version": "2.4.2",
  "scripts": {
    "build": "export VITE_FIREBASE_DATA_SOURCE=live && vite build",
    "build:sandbox": "export VITE_FIREBASE_DATA_SOURCE=sandbox && vite build",
    "build:levante-dev": "vite build -m LEVANTE",
    "dev": "export VITE_FIREBASE_DATA_SOURCE=sandbox && vite --force --host",
    "dev:live": "export VITE_FIREBASE_DATA_SOURCE=live && vite --force --host",
    "dev:levante": "vite --force --host -m LEVANTE",
    "dev:emulate": "export VITE_FIREBASE_EMULATOR=true && vite --force --host",
    "deploy:levante-dev": "npm run build:levante-dev && firebase use levante-admin-dev && firebase deploy --only hosting --config levante-dev-firebase.json",
    "emulate:admin": "source firebase/emulate_admin.sh",
    "emulate:assessment": "source firebase/emulate_assessment.sh",
    "emulate:export": "source firebase/export_admin.sh && source firebase/export_assessment.sh",
    "emulate:start": "unset GOOGLE_APPLICATION_CREDENTIALS && npm run emulate:admin && npm run emulate:assessment",
    "emulate:stop": "ps -e | grep 'firebase emulators:start' | grep -v 'grep' | grep -v 'rg' | sed -e 's/^[ \t]*//' | tr -s ' ' | cut -d ' ' -f 1 | xargs kill -s INT",
    "cypress:open": "cypress open",
    "format": "prettier . --write --log-level=warn",
    "check-format": "prettier . --check",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "postversion": "GIT_REMOTE=$(git remote -v | grep \"yeatmanlab/roar-dashboard.git \\(push\\)\" | cut -f 1) && git push $GIT_REMOTE && git push $GIT_REMOTE --tags",
    "prepare": "husky install",
    "preversion": "npm run lint",
    "preview": "vite preview",
    "version": "npm run format && git add -A"
  },
  "dependencies": {
    "@bdelab/roam-fluency": "^1.11.12",
    "@bdelab/roar-firekit": "^4.9.1",
    "@bdelab/roar-letter": "^1.11.3",
    "@bdelab/roar-multichoice": "^1.11.2",
    "@bdelab/roar-pa": "^2.1.3",
    "@bdelab/roar-sre": "^1.13.4",
    "@bdelab/roar-swr": "^1.9.3",
    "@bdelab/roar-utils": "^1.2.1",
    "@bdelab/roar-vocab": "^1.7.2",
    "@bdelab/roav-ran": "^1.0.12",
    "@sentry/browser": "^7.107.0",
    "@sentry/integrations": "^7.107.0",
    "@sentry/vite-plugin": "^2.14.2",
    "@sentry/vue": "^7.107.0",
    "@sentry/wasm": "^7.107.0",
    "@tanstack/vue-query": "^4.35.7",
    "@vuelidate/core": "^2.0.0-alpha.40",
    "@vuelidate/validators": "^2.0.0-alpha.28",
    "@vueuse/core": "^10.7.1",
    "@vueuse/head": "^1.0.13",
    "axios": "^1.5.1",
    "buffer": "^6.0.3",
    "chart.js": "^4.3.3",
    "core-tasks": "^1.0.0-beta.3",
    "dotenv": "^16.3.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "lodash": "^4.17.21",
    "marked": "^7.0.3",
    "papaparse": "^5.3.2",
    "pinia": "^2.0.33",
    "pinia-plugin-persistedstate": "^3.1.0",
    "primeflex": "^3.1.3",
    "primeicons": "^5.0.0",
    "primevue": "^3.32.1",
    "sharp": "^0.33.3",
    "showdown": "^2.1.0",
    "survey-vue3-ui": "^1.9.131",
    "vega": "^5.26.1",
    "vega-embed": "^6.23.0",
    "vega-lite": "^5.16.3",
    "video.js": "^8.10.0",
    "vite-plugin-node-polyfills": "^0.9.0",
    "vue": "^3.2.25",
    "vue-country-flag-next": "^2.3.2",
    "vue-draggable-next": "^2.2.1",
    "vue-google-maps-community-fork": "^0.3.1",
    "vue-i18n": "^9.9.1",
    "vue-router": "^4.1.6",
    "vue3-text-clamp": "^0.1.2",
    "zipson": "^0.2.12"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.196",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "cypress": "^13.6.3",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "prettier": "3.0.3",
    "sass": "^1.50.1",
    "vite": "^4.5.3",
    "vite-plugin-favicons-inject": "^2.2.0"
  }
}
