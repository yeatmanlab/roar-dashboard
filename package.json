{
  "name": "roar-dashboard",
  "private": true,
  "version": "3.1.0",
  "type": "module",
  "scripts": {
    "build:dev": "export VITE_LEVANTE=TRUE && export VITE_FIREBASE_PROJECT=DEV && vite build",
    "build:prod": "export VITE_LEVANTE=TRUE && vite build",
    "dev": "export VITE_LEVANTE=TRUE && export VITE_FIREBASE_PROJECT=DEV && vite --force --host",
    "local:prod": "export VITE_LEVANTE=TRUE && export VITE_FIREBASE_PROJECT=PROD && vite --force --host",
    "dev:pc": "powershell -Command \"$env:VITE_LEVANTE='TRUE'; $env:VITE_FIREBASE_PROJECT='DEV'; vite --force --host\"",
    "deploy:dev": "npm run build:dev && npx firebase use levante-admin-dev && npx firebase deploy --only hosting --config levante-firebase.json",
    "deploy:prod": "npm run build:prod && npx firebase use levante-admin-prod && npx firebase deploy --only hosting --config levante-firebase.json",
    "cypress:open": "cypress open",
    "format": "prettier . --write --log-level=warn",
    "generate-pwa-assets": "pwa-assets-generator",
    "check-format": "prettier . --check",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "postversion": "GIT_REMOTE=$(git remote -v | grep \"yeatmanlab/roar-dashboard.git \\(push\\)\" | cut -f 1) && git push $GIT_REMOTE && git push $GIT_REMOTE --tags",
    "preversion": "npm run lint",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "version": "npm run format && git add -A"
  },
  "dependencies": {
    "@bdelab/roar-pa": "2.2.4",
    "@bdelab/roar-sre": "1.15.18",
    "@bdelab/roar-swr": "1.12.11",
    "@bdelab/roar-utils": "^1.2.1",
    "@levante-framework/core-tasks": "^1.0.0-beta.35",
    "@levante-framework/firekit": "^9.2.11",
    "@primevue/core": "^4.2.4",
    "@primevue/themes": "^4.2.4",
    "@sentry/browser": "^8.0.0",
    "@sentry/integrations": "^7.114.0",
    "@sentry/vite-plugin": "^2.16.1",
    "@sentry/vue": "^7.114.0",
    "@tanstack/vue-query": "^5.54.2",
    "@tanstack/vue-query-devtools": "^5.54.2",
    "@unhead/vue": "^1.9.16",
    "@vuelidate/core": "^2.0.0-alpha.40",
    "@vuelidate/validators": "^2.0.0-alpha.28",
    "@vueuse/core": "^10.7.1",
    "axios": "^1.5.1",
    "buffer": "^6.0.3",
    "chart.js": "^4.3.3",
    "dompurify": "^3.1.6",
    "dotenv": "^16.3.1",
    "html2canvas": "^1.4.1",
    "http-status-codes": "^2.3.0",
    "jspdf": "^2.5.1",
    "lodash": "^4.17.21",
    "marked": "^7.0.3",
    "papaparse": "^5.3.2",
    "pinia": "^2.0.33",
    "pinia-plugin-persistedstate": "^3.1.0",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.2.4",
    "sharp": "^0.33.3",
    "showdown": "^2.1.0",
    "survey-core": "^2.0.2",
    "survey-vue3-ui": "^2.0.2",
    "vega": "^5.26.1",
    "vega-embed": "^6.23.0",
    "vega-lite": "^5.16.3",
    "video.js": "^8.10.0",
    "vue": "^3.2.25",
    "vue-draggable-next": "^2.2.1",
    "vue-google-maps-community-fork": "^0.3.1",
    "vue-i18n": "^9.9.1",
    "vue-router": "^4.1.6",
    "vue3-text-clamp": "^0.1.2",
    "workbox-precaching": "^7.1.0",
    "zipson": "^0.2.12"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.5",
    "@testing-library/vue": "^8.1.0",
    "@types/lodash": "^4.14.196",
    "@unhead/addons": "^1.9.16",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitest/coverage-istanbul": "^2.0.5",
    "cypress": "^13.14.1",
    "cypress-fs": "^0.2.5",
    "cypress-vite": "^1.5.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "happy-dom": "^14.12.3",
    "husky": "^9.1.6",
    "nanoid": "^5.0.7",
    "prettier": "3.0.3",
    "sass": "^1.50.1",
    "vite": "^5.4.2",
    "vite-plugin-favicons-inject": "^2.2.0",
    "vite-plugin-mkcert": "^1.17.5",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vitest": "^2.0.5",
    "workbox-build": "^7.1.1",
    "workbox-core": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0"
  }
}
